{
  "name": "backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "scripts": {
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "prebuild:ts": "npm run predev",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "predev": "rm -rf dist",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "export $(grep -v '^#' .env | xargs); fastify start --debug --debug-port -P --ignore-watch='.ts$ data' -w -l info -P dist/app.js",
    "migrate": "export $(grep -v '^#' .env | xargs); npx sequelize-cli db:migrate",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnlup/fastify-traps": "^2.0.0",
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/env": "^4.2.0",
    "@fastify/routes": "^5.1.0",
    "@fastify/schedule": "^4.1.1",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.6.0",
    "@fastify/type-provider-typebox": "^3.2.0",
    "@fastify/websocket": "^8.1.0",
    "@sinclair/typebox": "^0.28.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "croner": "^6.0.5",
    "date-fns": "^2.30.0",
    "esbuild": "^0.18.10",
    "eslint": "^8.43.0",
    "eslint-config-standard": "^17.1.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "lodash": "^4.17.21",
    "onoff": "^6.0.3",
    "rrule": "^2.7.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "toad-scheduler": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "@types/ws": "^8.5.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "sequelize-cli": "^6.6.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.4.5"
  },
  "directories": {
    "test": "test"
  }
}
